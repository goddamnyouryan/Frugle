<!DOCTYPE html>
<html>
<head>
	<!--[if lt IE 9]>
		<script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
	<![endif]-->
	<!--[if lte IE 7]>
	        <%= stylesheet_link_tag 'ie-print' %>
	<![endif]-->
  <title>Frugle | <%= content_for?(:title) ? yield(:title) : "" %></title>
	<% if params[:controller] == "frugles" && params[:action] == "print" %>
			<%= raw(GMap.header) %>
			<%= raw(@map.to_html) %>
	<% end %>
	<!--[if !IE]><!-->
	        <%= stylesheet_link_tag "print" %>
	 <!--<![endif]-->
  <%= javascript_include_tag :defaults %>
	<%= csrf_meta_tag %>
  <%= yield(:head) %>
<link rel="shortcut icon" href="/favicon.ico" type="image/ico" />
</head>
<body Onload="setTimeout('print()', 1000);">
	
	<div class="clear"></div>

	<div id="container">
      <%= yield %>
  </div>
	
	<script type="text/javascript">

	  var _gaq = _gaq || [];
	  _gaq.push(['_setAccount', 'UA-22646449-1']);
	  _gaq.push(['_trackPageview']);

	  (function() {
	    var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
	    ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
	    var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
	  })();

	</script>
	
</body>
</html>
