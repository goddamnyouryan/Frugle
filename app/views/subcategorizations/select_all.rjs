@user_subcategories.each do |subcategory|
  page.replace_html "subcategory_#{subcategory.id}", "#{link_to subcategory.title, subcategorization_path(current_user, :subcategory_id => subcategory.id), :method => :delete, :remote => true, :style => 'background:#ffc90e;'}"
end
page.replace_html "frugles", :partial => 'neighborhoods/small_frugle', :collection => @results, :as => :frugle
page.replace_html "frugles_count", "Frugles - #{pluralize(@results.count, 'result')}"
page << @map.clear_overlays
for marker in @markers
  page << @map.add_overlay(marker)
end