<% title "Home | #{@neighborhood.name} - #{@frugle_count.count} Frugles" %>

<% if user_signed_in? %>

	<% if current_user.role == "business" %>
		<%= render :partial => 'business' %>
	<% else %>
		<%= render :partial => 'user' %>
	<% end %>
	
<% else %>

	<%= render :partial => 'logged_out' %>

<% end %>

